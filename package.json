{
  "name": "gomoku",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "cypress:open": "cypress open",
    "cypress:prepare": "yarn run build && vite preview --port 3999",
    "cypress:test": "cypress run"
  },
  "dependencies": {
    "@types/node": "^17.0.21",
    "classnames": "^2.3.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.2",
    "reset-css": "^5.0.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-react": "^1.0.7",
    "cypress": "^9.5.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-xo": "^0.39.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "sass": "^1.49.9",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vite-plugin-checker": "^0.4.2"
  },
  "prettier": {
    "printWidth": 120,
    "proseWrap": "preserve",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "tabWidth": 2,
    "arrowParens": "avoid",
    "trailingComma": "es5"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx,json,md}": "yarn run lint:check"
  }
}
